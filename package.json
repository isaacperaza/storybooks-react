    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^4.14.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "next": "^11.1.0",
    "nyc": "^11.3.0",
    "prismjs": "^1.13.0",
    "raw-loader": "^0.5.1",
